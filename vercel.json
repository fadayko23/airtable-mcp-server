{
  "version": 2,
  "builds": [
    {
      "src": "api/sse.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/api/test" },
    { "src": "/test", "dest": "/api/test" },
    { "src": "/sse", "dest": "/api/sse" }
  ]
}


